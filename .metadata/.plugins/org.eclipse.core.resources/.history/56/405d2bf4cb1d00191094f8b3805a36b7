typedef struct elemento{
	int info;
	struct elemento * link;
	}FormatoNo;


typedef struct lista{
	FormatoNo * IndIniLst;
	FormatoNo * IndFimLst;
}FormatoLst;


typedef struct pilha{
	FormatoLst * l;
}FormatoPlha;



